.add{
  font-size: 80rpx;
  color: #888;
  position: fixed;
  bottom: 0rpx;
  left: 50%;
  transform: translateX(-50%);
  z-index: 100;
}
.list{
  height: 500rpx;
  display: flex;
  flex-direction: column;
  margin-top: 50rpx;
  justify-content:space-around;
}
.title{
  width: 100%;
  height: 150rpx;
  line-height: 150rpx;
  text-align: center;
  font-weight: bold;
  color: #888;
}
.addbut{
  font-weight: bold;
  position: absolute;
  bottom: 80rpx;
  font-size: 28rpx;
  left: 49rpx;
}
.img{
  width: 50rpx;
  height: 50rpx;
}
.upbut{
  font-weight: bold;
  position: absolute;
  bottom: 150rpx;
  font-size: 28rpx;
  left: 49rpx;
}