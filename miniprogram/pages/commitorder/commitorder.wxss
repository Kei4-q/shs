/* pages/cart/cart.wxss */
.van-card{
  width: 700rpx;
  }
  .h{
    height: 930rpx;
  }
  .van-submit-bar{
    height: 110rpx;
    z-index: 100;
  }
  .l0{
    position: absolute;
    left: 0;
    top: 40rpx;
  }
  .empty{
    height: 100rpx;
    width: 100%;
    line-height: 100rpx;
    text-align: center;
    font-weight: bold;
    color: #888;
  }
  .bottom{
    position: fixed;
    width: 100%;
    height: 110rpx;
    bottom: 30rpx;
    background-color: #fff;
  }
  .price{
    height: 110rpx;
    font-weight: bold;
    float: left;
    font-size: 35rpx;
    line-height: 110rpx;
    margin-left: 200rpx;
  }
  .number{
    font-size: 30rpx;
    float: left;
    margin-left: 40rpx;
    line-height: 118rpx;
  }
  .total{
    float: right;
    line-height: 118rpx;
    color: red;
    font-weight:500;
    font-size: 30rpx;
  }
  .button{
    float: right;
    line-height: 100rpx;
    margin-right: 6rpx;
  }
  .van-cell{
    margin:10rpx 0;
  }
  .wrapper {
    display: flex;
    text-align: center;
    align-items: center;
    justify-content: center;
    height: 100%;
  }
  .block {
    line-height: 200rpx;
    width: 600rpx;
    height: 200rpx;
    background-color: #fff;
  }
.showbutton{
  height:150rpx;
  margin-left: 50rpx;
}